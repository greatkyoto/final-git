<div class ="container">
     <h1>メモ新規作成</h1>
      <%= form_tag ("/create") do %>
          <div class="form-group">
               <%= label_tag "form-title", "タイトル" %>
               <%= text_field :memos, :title, id:"form-title", class:"form-control" %>
          </div>
          <div class="form-group">
               <%= label_tag "form-content", "メモ内容" %>
               <%= text_field :memos, :body, id:"form-content" , class:"form-control"%>
          </div>
          <%= submit_tag "作成" , class:"btn btn-primary"%>
     <% end %>
</div>